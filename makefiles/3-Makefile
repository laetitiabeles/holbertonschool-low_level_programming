CC = gcc
SRC = main.c school.c
OBJ = school.o main.o
NAME = school
RM = rm -f

all : school

school.o : school.c
	$(CC) -o $(OBJ) -c $(SRC) $(NAME)

main.o : main.c
	$(CC) -o $(OBJ) -c $(SRC) $(NAME)

oclean : $(OBJ)
	$(RM) *.o

clean : $(RM
	$(RM) *~

fclean : clean $(NAME)
	$(RM) $(NAME)

re: fclean all

.PHONY: all clean fclean re
